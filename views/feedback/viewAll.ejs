<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/4.3.0/mdb.min.js"></script>
<script src="/js/home.js"></script>
<link href="/css/feedback/viewAll.css" rel="stylesheet" />

<div class="container">
    <% for(let feedback of feedbacks) { %>
          <div class="card" style="width: 18rem; height:22rem;">
            <div class="card-header">
                <p class="card-text mt-2"><small class="text-muted" >Total Responses: 224</small></p>
              </div>
        <div class="card-body" style="padding-bottom: 0;">
          <h5 class="card-title"><%=feedback.name%></h5>
          <p class="card-text"><%=feedback.description%></p>
          <a href="/feedback/<%=feedback._id%>" class="btn btn-primary">View Details</a>
          
        </div>
        <div class="card-footer text-muted">
            <p class="card-text mt-2"><small class="text-muted" >Created : <%=feedback.createdAt.toString().substring(0,16)%></small></p>
            
          </div>
      </div>
        <% } %>
  
